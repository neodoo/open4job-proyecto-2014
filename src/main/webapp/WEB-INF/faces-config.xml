<?xml version="1.0" encoding="UTF-8"?>

<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<application>

		<locale-config>
			<default-locale>es</default-locale>
		</locale-config>

		<resource-bundle>
			<base-name>es.opensigad.etiquetasAlumno</base-name>
			<var>etqAlm</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>es.opensigad.etiquetasAlumnoMatricula</base-name>
			<var>etqAlmMat</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>es.opensigad.etiquetasAlumnoSeguimiento</base-name>
			<var>etqAlmSeg</var>
		</resource-bundle>
		
		<resource-bundle>
			<base-name>es.opensigad.etiquetasNotas</base-name>
			<var>etqAlmNotas</var>
		</resource-bundle>

	</application>

</faces-config>